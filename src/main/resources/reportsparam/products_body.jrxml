<queryString>
   <![CDATA[
       SELECT p.id AS id,
              p.product_code AS product_code,
              p.name AS name,
              COALESCE(c.name,'') AS category,
              COALESCE(p.unit,'') AS unit,
              p.created_at AS created_at
       FROM products p
       LEFT JOIN categories c ON c.id = p.category_id
       WHERE 1=1 $P!{P_QUERY}
   ]]>
</queryString>

